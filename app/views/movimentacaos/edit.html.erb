<% content_for :title, "Editing movimentacao" %>

<div class="movimentacoes-container">
  <h1>Editing movimentacao</h1>
  <div class="movimentacao-card">
    <%= render "form", movimentacao: @movimentacao %>
  </div>
  <div class="movimentacao-links voltar-home">
